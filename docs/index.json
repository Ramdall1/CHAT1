{
  "project": {
    "name": "chat-bot-enterprise",
    "version": "5.1.0",
    "description": "Sistema de Chat Bot autónomo, estable y escalable con IA conversacional y gestión completa de contactos",
    "main": "src/main.js",
    "type": "module",
    "engines": {
      "node": ">=18.0.0",
      "npm": ">=9.0.0"
    },
    "exports": {
      ".": {
        "import": "./src/main.js",
        "types": "./types/index.d.ts"
      },
      "./core": {
        "import": "./src/core/index.js"
      },
      "./utils": {
        "import": "./src/utils/index.js"
      },
      "./config": {
        "import": "./src/config/index.js"
      }
    },
    "imports": {
      "#core/*": "./src/core/*.js",
      "#utils/*": "./src/utils/*.js",
      "#config/*": "./src/config/*.js",
      "#types/*": "./src/types/*.js"
    },
    "scripts": {
      "start": "node src/main.js",
      "start:dev": "node --watch src/main.js",
      "start:debug": "node --inspect src/main.js",
      "start:legacy": "node src/main.js",
      "start:prod": "NODE_ENV=production pm2 start ecosystem.config.js",
      "stop": "pm2 stop ecosystem.config.js",
      "restart": "pm2 restart ecosystem.config.js",
      "test": "cross-env NODE_ENV=test jest --config=jest.config.js",
      "test:watch": "cross-env NODE_ENV=test jest --config=jest.config.js --watch",
      "test:coverage": "cross-env NODE_ENV=test jest --config=jest.config.js --coverage",
      "test:fast": "cross-env NODE_ENV=test jest --config=jest.fast.config.js",
      "test:fast:watch": "cross-env NODE_ENV=test jest --config=jest.fast.config.js --watch",
      "test:quick": "cross-env NODE_ENV=test jest --config=jest.fast.config.js --bail --maxWorkers=1",
      "test:unit:fast": "cross-env NODE_ENV=test jest tests/unit --config=jest.fast.config.js --bail",
      "test:real:fast": "cross-env NODE_ENV=test jest tests/real-unit --config=jest.fast.config.js --bail",
      "test:smart": "node scripts/smart-test.js",
      "test:parallel": "cross-env NODE_ENV=test CI_PARALLEL=true jest --config=jest.config.js --maxWorkers=75%",
      "test:integration": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' npx jest tests/integration",
      "test:integration:watch": "cross-env NODE_ENV=test NODE_OPTIONS='--experimental-vm-modules --no-warnings' jest --config=jest.integration.config.js --watch",
      "test:unit": "NODE_OPTIONS='--experimental-vm-modules --no-warnings' npx jest tests/unit",
      "test:e2e": "npx playwright test",
      "test:all": "npm run test && npm run test:e2e",
      "test:scoring": "node test-scoring-system.js",
      "test:cleanup": "node cleanup-test-reports.js",
      "test:quality": "node scripts/verify-quality.js",
      "test:benchmark": "node scripts/jest-benchmark.js",
      "test:thresholds": "node scripts/progressive-thresholds.js",
      "test:thresholds:apply": "node scripts/progressive-thresholds.js --apply",
      "coverage:report": "node scripts/coverage-report.js",
      "ci:local": "node scripts/local-ci.js",
      "notify:quality": "node scripts/quality-notifications.js",
      "notify:configure": "node scripts/quality-notifications.js --configure",
      "notify:config": "node scripts/quality-notifications.js --show-config",
      "setup:quality": "node scripts/setup-quality-system.js",
      "health": "curl -s http://localhost:3000/health | jq",
      "stats": "curl -s http://localhost:3000/stats | jq",
      "lint": "eslint .",
      "lint:fix": "eslint . --ext .js,.mjs,.cjs --fix",
      "format": "prettier --write .",
      "format:check": "prettier --check .",
      "type-check": "tsc --noEmit",
      "build": "npm run lint && npm run type-check && npm run test",
      "build:docker": "docker build -t chatbot-enterprise .",
      "docker:up": "docker-compose up -d",
      "docker:down": "docker-compose down",
      "docker:logs": "docker-compose logs -f",
      "setup": "npm install && npm run prepare",
      "clean": "rm -rf node_modules package-lock.json && npm install",
      "clean:cache": "npm cache clean --force",
      "logs": "pm2 logs",
      "monitor": "pm2 monit",
      "backup": "node scripts/backup.js",
      "maintenance": "node scripts/cleanup-maintenance.js",
      "maintenance:dry-run": "echo 'Ejecutando mantenimiento en modo simulación...' && node scripts/cleanup-maintenance.js --dry-run",
      "migrate": "node scripts/migrate.js",
      "seed": "node scripts/seed.js",
      "prepare": "npx husky",
      "security:audit": "npm audit --audit-level=moderate",
      "imports:check": "node maintenance-check.js",
      "imports:fix": "node fix-imports-smart.js",
      "imports:circular": "node detect-circular-imports.js",
      "imports:syntax": "node validate-import-syntax.js",
      "imports:availability": "node check-module-availability.js",
      "security:fix": "npm audit fix",
      "deps:check": "npm outdated",
      "deps:update": "npm update"
    },
