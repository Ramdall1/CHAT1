{
    "name": "chat1",
    "version": "1.0.0",
    "description": "",
    "main": "src/main.js",
    "type": "module",
    "imports": {
        "#core/*": "./src/core/*",
        "#database/*": "./src/database/*",
        "#services/*": "./src/services/*",
        "#middleware/*": "./src/middleware/*",
        "#routes/*": "./src/api/routes/*",
        "#components/*": "./src/components/*",
        "#app/*": "./src/app/*",
        "#infrastructure/*": "./src/infrastructure/*",
        "#shared/*": "./src/shared/*",
        "#repositories/*": "./src/repositories/*"
    },
    "scripts": {
        "start": "pkill -f 'node.*main.js' || true && pkill -f 'node.*3000' || true && pkill -f 'node.*3001' || true && sleep 3 && echo 'ðŸ§¹ Limpiando procesos anteriores...' && ps aux | grep 'node.*main.js' | grep -v grep | awk '{print $2}' | xargs -r kill -9 2>/dev/null || true && sleep 2 && echo 'ðŸš€ Iniciando nueva instancia...' && NODE_ENV=production node src/main.js",
        "dev": "pkill -f 'node.*main.js' || true && pkill -f 'node.*3000' || true && pkill -f 'node.*3001' || true && sleep 2 && NODE_ENV=development nodemon src/main.js",
        "build": "tsc",
        "build:watch": "tsc --watch",
        "test": "NODE_ENV=test jest",
        "test:unit": "NODE_ENV=test jest --testPathPatterns=tests/unit",
        "test:integration": "NODE_ENV=test jest --testPathPatterns=tests/integration",
        "test:e2e": "NODE_ENV=test jest --testPathPatterns=tests/e2e",
        "test:coverage": "NODE_ENV=test jest --coverage",
        "test:watch": "NODE_ENV=test jest --watch",
        "lint": "eslint 'src/**/*.{js,ts}' 'client/**/*.js'",
        "lint:fix": "eslint 'src/**/*.{js,ts}' 'client/**/*.js' --fix",
        "format": "prettier --write 'src/**/*.{js,ts,json}' 'client/**/*.{js,json}'",
        "format:check": "prettier --check 'src/**/*.{js,ts,json}' 'client/**/*.{js,json}'",
        "type-check": "tsc --noEmit",
        "clean": "rm -rf dist coverage .eslintcache",
        "migrate": "node scripts/migrate.js",
        "migrate:status": "node scripts/migrate-status.js",
        "db:seed": "node scripts/seed.js",
        "health:check": "curl -f http://localhost:3000/health || echo 'Server not running'"
    },
